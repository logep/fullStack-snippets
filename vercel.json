{
  "builds": [
    { "src": "angular/", "use": "@vercel/node" },
    { "src": "react/", "use": "@vercel/node" },
    { "src": "vue/", "use": "@vercel/node" },
    { "src": "rust/", "use": "@vercel/rust" }
  ],
  "routes": [
    {
      "src": "/angular/(.*)",
      "dest": "/angular/$1"
    },
    {
      "src": "/react/(.*)",
      "dest": "/react/$1"
    },
    {
      "src": "/vue/(.*)",
      "dest": "/vue/$1"
    },
    {
      "src": "/rust/(.*)",
      "dest": "/rust/$1"
    }
  }
}
